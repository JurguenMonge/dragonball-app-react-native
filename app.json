{
  "expo": {
    "name": "dragonball-app",
    "slug": "dragonball-app",
    "scheme": "dragonball",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/splash.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/images/icon.jpg"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
